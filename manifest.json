{
	"manifest_version": 2,
	"name": "Clipboard to Title",
	"description": "This extension copy clipboard to title.",
	"version": "1",
	"content_scripts": [
		{
			"matches": ["http://*/*","https://*/*"],
			"js"     : ["jquery.min.js","main.js"]
		}
	],

	"web_accessible_resources": [
		"/*"
	],
	"permissions": [
		"clipboardRead"
	]
}